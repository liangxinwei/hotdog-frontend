{
  "name": "hotdog-frontend",
  "version": "1.0.0",
  "description": "个人测试项目",
  "author": "liangxinwei <liangxinwei@spacebox.fun>",
  "license": "MIT",
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "preview": "node build/index.js --preview",
    "lint": "eslint --fix --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "deploy": "npm run build && expect build/deploy.sh",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "port": 7000
  },
  "keywords": [
    "hotdog",
    "Vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liangxinwei/hotdog-frontend.git"
  },
  "dependencies": {
    "autoprefixer": "^9.7.5",
    "axios": "0.19.0",
    "clipboard": "2.0.4",
    "dayjs": "^1.8.16",
    "lodash.clonedeep": "^4.5.0",
    "vant": "^2.5.8",
    "vue": "2.6.10",
    "vue-router": "3.0.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "^3.9.1",
    "@vue/cli-plugin-unit-jest": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "chalk": "2.4.2",
    "connect": "3.6.6",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "8.1.5",
    "runjs": "^4.3.2",
    "postcss-px-to-viewport": "^1.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
